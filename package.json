    "css"
  ],
  "author": "Hafiz Muhammad Bilal <hafiz.mbilal@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hmbilal/flag-icons/issues"
  },
  "homepage": "https://github.com/hmbilal/flag-icons",
  "devDependencies": {
    "node-sass": "^7.0.3"
  },
  "scripts": {
    "build": "yarn build:dev && yarn build:min",
    "build:dev": "node-sass scss/flag-icons.scss > css/flag-icons.css",
