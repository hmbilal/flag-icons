{
  "name": "flag-icons",
  "version": "1.2",
  "description": "Flags of world countries",
  "repository": {
    "type": "git",
    "url": "https://github.com/hmbilal/flag-icons.git"
  },
  "keywords": [
    "flags",
    "world",
    "country",
    "flag",
